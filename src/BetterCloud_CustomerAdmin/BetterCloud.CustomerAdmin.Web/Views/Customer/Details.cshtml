@model BetterCloud.CustomerAdmin.Common.DataObjects.CustomerDO

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div>
    <h4>Customer: @Html.DisplayFor(model => model.CustomerId)</h4>
